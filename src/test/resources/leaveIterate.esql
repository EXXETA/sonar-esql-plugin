CREATE COMPUTE MODULE ProcessCardAccount

	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		DECLARE i INTEGER;
		SET i = 0;
		X : REPEAT
		  SET i = i + 1;
		
		  -- Some statements 1
		
		  IF i IN(2, 3) THEN
		    ITERATE X;
		  END IF;

		  IF i IN(2, 3) THEN
		    ITERATE X;
		  END IF;
		
		  -- Einige Anweisungen 2
		
		UNTIL
		  i >= 4
		END REPEAT X;
		X : REPEAT
		  SET i = i + 1;
		
		  -- Some statements 1
		
		  IF i IN(2, 3) THEN
		    LEAVE X;
		  END IF;

		  IF i IN(2, 3) THEN
		    LEAVE X;
		  END IF;
		
		  -- Einige Anweisungen 2
		
		UNTIL
		  i >= 4
		END REPEAT X;
	END;

END MODULE;